{
    "name": "eslint-config-schaffrath",
    "version": "2.0.2",
    "main": "lib/configuration.js",
    "devDependencies": {
        "eslint": "6.8.0"
    },
    "scripts": {
        "lint": "npx eslint './**/*.js' --ignore-pattern './node_modules/**/*'",
        "prepublish": "npm run lint",
        "publish": "npm version ${npm_config_type}",
        "postpublish": "git push && git push --tags && npm publish"
    },
    "keywords": [
        "eslint",
        "highlighter",
        "fehlerkorrektur"
    ],
    "repository": {
        "type": "git",
        "url": "git://github.com/macmissionar/eslint-config-schaffrath.git"
    }
}
